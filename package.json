{
    "name": "c-bot",
    "description": "An automation tool is to pull data",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=12.16.1"
    },
    "repository": "git+ssh://git@bitbucket.org/ccintegrationteam/c-bot.git",
    "keywords": [
        "NodeJS",
        "TypeScript",
        "express",
        "skeleton",
        "starter-kit",
        "bot"
    ],
    "homepage": "https://bitbucket.org/ccintegrationteam/c-bot.git#readme",
    "author": "Blue Sky SW Team <swteam@ccintegration.com>",
    "contributors": [
        {
            "name": "Phong V Lam",
            "email": "phong.lam@ccintegration.com",
            "url": "https://github.com/"
        }
    ],
    "scripts": {
        "compile": "tsc",
        "build": "cd ui-app && npm run build",
        "prod": "node ./dist/app/index.js",
        "test": "jest --coverage --runInBand",
        "test:watch": "jest --coverage --watchAll",
        "view:coverage": "serve coverage/lcov-report",
        "clean": "rm -rf coverage ./dist/**/*.js ./dist/**/*.map",
        "dev-be": "ts-node-dev --respawn --transpile-only ./app/index.ts",
        "dev-fe": "cd ui-app && npm start",
        "dev": "concurrently --kill-others-on-fail \"npm run dev-be\" \"npm run dev-fe\""
    },
    "dependencies": {
        "@types/express-serve-static-core": "^4.17.13",
        "axios": "^0.21.1",
        "body-parser": "^1.19.0",
        "compression": "^1.7.4",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~2.6.9",
        "dotenv": "^8.2.0",
        "exceljs": "^4.3.0",
        "express": "^4.17.1",
        "formidable": "^1.2.2",
        "googleapis": "^59.0.0",
        "helmet": "^3.23.3",
        "http-errors": "~1.6.3",
        "https": "^1.0.0",
        "json2csv": "^5.0.5",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.20",
        "mongodb": "^3.6.3",
        "mongoose": "^5.10.15",
        "morgan": "~1.9.1",
        "nodemailer": "^6.7.2",
        "redis": "^3.0.2",
        "sequelize": "^5.22.3",
        "socket.io": "^2.4.1",
        "socket.io-client": "^2.3.0",
        "swagger-ui-express": "^4.1.4",
        "ts-node-dev": "^1.0.0",
        "tslib": "^1.14.1",
        "winston": "^3.3.3",
        "xlsx": "^0.17.5"
    },
    "devDependencies": {
        "@shelf/jest-mongodb": "^1.2.3",
        "@types/compression": "^1.7.0",
        "@types/cookie-parser": "^1.4.2",
        "@types/cors": "^2.8.8",
        "@types/dotenv": "^8.2.0",
        "@types/express": "^4.17.9",
        "@types/formidable": "^1.0.31",
        "@types/helmet": "0.0.45",
        "@types/http-errors": "^1.8.0",
        "@types/jest": "^25.2.3",
        "@types/json2csv": "^5.0.1",
        "@types/jsonwebtoken": "^8.5.0",
        "@types/lodash": "^4.14.165",
        "@types/mongoose": "^5.10.4",
        "@types/morgan": "^1.9.2",
        "@types/node": "^14.14.8",
        "@types/nodemailer": "^6.4.4",
        "@types/redis": "^2.8.28",
        "@types/socket.io": "^2.1.13",
        "@types/socket.io-client": "^1.4.36",
        "@types/swagger-ui-express": "^4.1.2",
        "concurrently": "^7.0.0",
        "jest": "^27.4.7",
        "ts-jest": "^25.5.1",
        "typescript": "^3.9.7"
    }
}
